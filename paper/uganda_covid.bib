	language = {en},
	month = may,
	note = {Issue: 13297 Publication Title: IZA Discussion Papers},
	number = {13297},
	shorttitle = {Between a {Rock} and a {Hard} {Place}},
	title = {Between a {Rock} and a {Hard} {Place}: {Poverty} and {COVID}-19 in {Developing} {Countries}},
	url = {https://ideas.repec.org/p/iza/izadps/dp13297.html},
	urldate = {2022-04-09},
