	keywords = {Family, Female, Humans, Income, Male, Bangladesh, Adult, Young Adult, COVID-19, Food Supply, Pandemics, Rural Population, Quarantine, Coronavirus Infections, Pneumonia, Viral, Child, Preschool, Infant, Interrupted Time Series Analysis, Intimate Partner Violence, Mental Disorders, Mothers, Randomized Controlled Trials as Topic},
	language = {eng},
	month = nov,
	note = {Number: 11},
	number = {11},
