	issn = {1476-1645},
	journal = {The American Journal of Tropical Medicine and Hygiene},
	keywords = {Female, Humans, Male, Ethiopia, Adult, Middle Aged, Young Adult, COVID-19, Food Security, Food Supply, Burkina Faso, Diet, Feeding Behavior, Nigeria, Nutritional Status, Urban Population},
