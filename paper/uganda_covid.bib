	issn = {2071-1050},
	journal = {Sustainability},
	keywords = {COVID-19, food security, pandemic, food price, food production, household food security, income loss, input shortage, low-income, systematic review, vulnerable groups},
	language = {en},
	month = jan,
	note = {Number: 9 Publisher: Multidisciplinary Digital Publishing Institute},
	number = {9},
	pages = {5294},
	shorttitle = {Food {Security} and {COVID}-19},
	title = {Food {Security} and {COVID}-19: {A} {Systematic} {Review} of the {First}-{Year} {Experience}},
	url = {https://www.mdpi.com/2071-1050/13/9/5294},
