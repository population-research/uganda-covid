	journal = {Advances in Food Security and Sustainability},
	pages = {145--173},
	pmcid = {PMC7550085},
	pmid = {null},
	title = {Impacts of {COVID}-19 on food systems and poverty in {Nigeria}},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7550085/},
