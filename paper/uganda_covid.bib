	keywords = {comparative social policy, disaster and emergency, Local communities, poverty and inequality},
	language = {en},
	month = jun,
	note = {Number: 2 Publisher: SAGE Publications India},
	number = {2},
	pages = {152--172},
	shorttitle = {Social {Policy}, {COVID}-19 and {Impoverished} {Migrants}},
	title = {Social {Policy}, {COVID}-19 and {Impoverished} {Migrants}: {Challenges} and {Prospects} in {Locked} {Down} {India}},
