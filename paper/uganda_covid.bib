	language = {en},
	month = jan,
	note = {Number: 17 Publisher: Multidisciplinary Digital Publishing Institute},
	number = {17},
	pages = {9863},
	title = {Impacts of {COVID}-19 on {Diverse} {Farm} {Systems} in {Tanzania} and {South} {Africa}},
	url = {https://www.mdpi.com/2071-1050/13/17/9863},
	urldate = {2022-04-10},
