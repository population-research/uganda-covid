	pages = {S138--S168},
	shorttitle = {{COVID}-19 and the future of microfinance},
	title = {{COVID}-19 and the future of microfinance: evidence and insights from {Pakistan}},
	url = {https://doi.org/10.1093/oxrep/graa014},
