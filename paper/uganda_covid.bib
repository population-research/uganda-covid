	number = {3},
	pages = {484--504},
	shorttitle = {The utterly unforeseen livelihood shock},
	title = {The utterly unforeseen livelihood shock: {COVID}-19 and street vendor coping mechanisms in {Hanoi}, {Chiang} {Mai} and {Luang} {Prabang}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/sjtg.12396},
