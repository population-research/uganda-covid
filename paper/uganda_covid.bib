	journal = {Canadian Journal of Development Studies / Revue canadienne d'{\'e}tudes du d{\'e}veloppement},
	keywords = {India, employment, COVID-19, food security, informality, J21, J46, J64},
	month = apr,
	note = {Number: 1-2 Publisher: Routledge \_eprint: https://doi.org/10.1080/02255189.2021.1890003},
	number = {1-2},
	pages = {145--164},
	shorttitle = {Pandemic, informality, and vulnerability},
	title = {Pandemic, informality, and vulnerability: impact of {COVID}-19 on livelihoods in {India}},
	url = {https://doi.org/10.1080/02255189.2021.1890003},
