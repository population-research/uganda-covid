	institution = {World Bank},
	keywords = {Poverty, Mortality, Covid-19, Livelihoods, Welfare, Coronavirus, Pandemic Impact},
	language = {English},
	month = jun,
	note = {Accepted: 2020-06-18T14:58:08Z},
	shorttitle = {Lives and {Livelihoods}},
	title = {Lives and {Livelihoods}: {Estimates} of the {Global} {Mortality} and {Poverty} {Effects} of the {COVID}-19 {Pandemic}},
	type = {Working {Paper}},
	url = {https://openknowledge.worldbank.org/handle/10986/33938},
