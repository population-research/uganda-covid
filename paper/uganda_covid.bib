	pages = {105280},
	shorttitle = {The short-term economic effects of {COVID}-19 on low-income households in rural {Kenya}},
	title = {The short-term economic effects of {COVID}-19 on low-income households in rural {Kenya}: {An} analysis using weekly financial household data},
	url = {https://www.sciencedirect.com/science/article/pii/S0305750X20304071},
