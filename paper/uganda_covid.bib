	issn = {1475-2727},
	journal = {Public Health Nutrition},
	keywords = {Family Characteristics, Female, Humans, Male, Poverty, Bangladesh, Adult, Middle Aged, Socioeconomic Factors, COVID-19, Food Security, Rural Population, SARS-CoV-2, Household food security, Cross-Sectional Studies, Diet Surveys, Diet, Healthy, Household dietary diversity, Quarantine},
	language = {eng},
	month = apr,
	note = {Number: 5},
	number = {5},
	pages = {1079--1087},
	pmcid = {PMC8025083},
	pmid = {33317657},
	title = {Determinants of household food security and dietary diversity during the {COVID}-19 pandemic in {Bangladesh}},
	volume = {24},
