	keywords = {Bangladesh, Employment, Age groups, COVID 19, Food, Labor economics, Rural areas, Salaries},
	language = {en},
	month = oct,
	note = {Number: 10 Publisher: Public Library of Science},
	number = {10},
	pages = {e0240709},
	shorttitle = {{COVID}-19 induced economic loss and ensuring food security for vulnerable groups},
	title = {{COVID}-19 induced economic loss and ensuring food security for vulnerable groups: {Policy} implications from {Bangladesh}},
	url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0240709},
