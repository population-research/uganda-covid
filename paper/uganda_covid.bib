	keywords = {Myanmar, COVID-19, Food security, Agri-food system, Phone surveys, Rural livelihoods},
	language = {en},
	month = mar,
	pages = {103026},
	shorttitle = {Impacts of {COVID}-19 on agricultural production and food systems in late transforming {Southeast} {Asia}},
	title = {Impacts of {COVID}-19 on agricultural production and food systems in late transforming {Southeast} {Asia}: {The} case of {Myanmar}},
