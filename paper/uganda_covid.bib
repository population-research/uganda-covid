	pmcid = {PMC7447230},
	pmid = {32857955},
	shorttitle = {Immediate impact of stay-at-home orders to control {COVID}-19 transmission on socioeconomic conditions, food insecurity, mental health, and intimate partner violence in {Bangladeshi} women and their families},
	title = {Immediate impact of stay-at-home orders to control {COVID}-19 transmission on socioeconomic conditions, food insecurity, mental health, and intimate partner violence in {Bangladeshi} women and their families: an interrupted time series},
