	keywords = {Child, Family Characteristics, Female, Humans, India, Longitudinal Studies, COVID-19, Food Supply, Pandemics, SARS-CoV-2, Communicable Disease Control, Food Insecurity, Cross-Sectional Studies, public health, Adaptation, Psychological, nutrition},
